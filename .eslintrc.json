{
  "parser": "babel-eslint",
  "env": { "browser": true, "jest": true },
  "extends": ["airbnb-base"],
  "rules": {
    "indent": ["error", 4],
    "comma-dangle": "off",
    "one-var": "off",
    "one-var-declaration-per-line": "off",
    "eol-last": "off",
    "no-param-reassign": "off",
    "class-methods-use-this": "off",

    "no-console": "off"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  }
}
